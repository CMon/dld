#ifndef __3DPOINT_H
#define __3DPOINT_H
class ThreeDPoint
{
	public:
		int	x;
		int	y;
		int	z;
};

class ThreeDFloatPoint
{
	public:
		float	x;
		float	y;
		float	z;
};
#endif
